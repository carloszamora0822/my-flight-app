{
    "name": "my-flight-app",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev": "concurrently \"npm run start:client\" \"npm run start:server\"",
        "start:client": "cd client && npm start",
        "start:server": "cd server && npm run dev",
        "build": "cd client && npm run build && cp -R build ../build"
    },
    "devDependencies": {
        "concurrently": "^8.2.2"
    },
    "dependencies": {
        "axios": "^1.7.9",
        "cors": "^2.8.5",
        "date-fns-tz": "^3.2.0",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "mongodb": "^6.13.1"
    }
}
